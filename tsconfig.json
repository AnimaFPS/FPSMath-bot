{
  "compilerOptions": {
    "target": "es2019",
    "module": "CommonJS",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "allowJs": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*", "./src/*"]
}
