{
	"name": "fpsmath",
	"private": true,
	"version": "3.0.0",
	"homepage": "https://fpsmath.animafps.xyz",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/AnimaFPS/FPSMath/issues"
	},
	"author": {
		"name": "Anima",
		"email": "animafps@pm.me"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://github.com/animafps/fpsmath.git"
	},
	"scripts": {
		"test": "jest",
		"dev": "nodemon src/index.ts --project src/tsconfig.json ",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write {src,tests}/**/*.ts",
		"build": "tsc -b src",
		"clean": "tsc -b src --clean",
		"watch": "tsc -b src -w",
		"commit": "git-cz --signoff",
		"cz": "git-cz --signoff",
		"update": "yarn upgrade-interactive",
		"sversion": "standard-version",
		"start": "ts-node src/index.ts"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.1.4",
		"@commitlint/config-conventional": "^12.1.4",
		"@sapphire/eslint-config": "^3.2.2",
		"@sapphire/prettier-config": "^1.1.6",
		"@sapphire/ts-config": "^2.3.2",
		"@types/dotenv": "^8.2.0",
		"@types/eslint-plugin-prettier": "^3.1.0",
		"@types/jest": "^26.0.24",
		"@types/node": "^16.3.1",
		"@types/ws": "^7.4.6",
		"@typescript-eslint/eslint-plugin": "^4.28.3",
		"@typescript-eslint/parser": "^4.28.3",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"husky": "^7.0.1",
		"jest": "^27.0.6",
		"lint-staged": "^11.0.0",
		"nodemon": "^2.0.12",
		"prettier": "^2.3.2",
		"pretty-quick": "^3.1.1",
		"standard-version": "^9.3.0",
		"ts-jest": "^27.0.3",
		"ts-node": "^10.1.0",
		"typescript": "^4.3.5"
	},
	"dependencies": {
		"@discordjs/collection": "^0.2.0",
		"@sapphire/decorators": "^2.1.5",
		"@sapphire/framework": "^1.0.2",
		"@sapphire/plugin-logger": "^1.0.13",
		"@sentry/node": "^6.9.0",
		"discord.js": "^12.5.3",
		"dotenv": "^10.0.0",
		"tslib": "^2.3.0"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	}
}
