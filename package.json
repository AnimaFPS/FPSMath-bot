{
  "name": "fpsmath",
  "version": "1.1.1",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint .",
    "start": "node index.js",
    "lint:fix": "eslint --fix .",
    "commit": "lint-staged && cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnimaFPS/FPSMath.git"
  },
  "keywords": [],
  "author": "AnimaFPS",
  "license": "MIT",
  "description": "A Discord bot designed to convert sensitivities, fovs, mouse feel across themselves and preset games",
  "dependencies": {
    "common-tags": "^1.8.0",
    "discord.js": "^12.5.1",
    "discord.js-commando": "github:discordjs/Commando",
    "mathjs": "^8.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5",
    "endOfLine": "lf",
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "**": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
